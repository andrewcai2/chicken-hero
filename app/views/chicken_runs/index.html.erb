<p id="notice"><%= notice %></p>

<h1>Chicken Runs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chicken_runs.each do |chicken_run| %>
      <tr>
        <td><%= link_to 'Show', chicken_run %></td>
        <td><%= link_to 'Edit', edit_chicken_run_path(chicken_run) %></td>
        <td><%= link_to 'Destroy', chicken_run, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

